{
  "name": "aiwebui",
  "version": "2.15.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node scripts/update-version.js",
    "start": "ng serve",
    "prebuild": "node scripts/update-version.js",
    "build": "ng build",
    "prebuild:prod": "node scripts/update-version.js",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "pretest": "node scripts/update-version.js",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:check": "ng lint --quiet",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "lint:arch": "depcruise src --config .dependency-cruiser.js",
    "lint:all": "npm run lint && npm run lint:scss && npm run lint:arch",
    "update-version": "node scripts/update-version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "compromise": "^14.14.4",
    "highlight.js": "^11.11.1",
    "marked": "^16.4.0",
    "ngx-cookie-service": "^20.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.3.0",
    "dependency-cruiser": "^17.2.0",
    "eslint": "^9.35.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "stylelint": "^16.25.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.12.1",
    "typescript": "~5.8.0",
    "typescript-eslint": "8.40.0"
  }
}
